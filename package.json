{
  "name": "code-graph",
  "displayName": "Code Graph",
  "description": "Visualize changed files and their dependencies as an interactive graph",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-graph.showGraph",
    "onCommand:code-graph.showGraphForCommit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-graph.showGraph",
        "title": "Code Graph: Show Dependency Graph",
        "icon": "$(graph)"
      },
      {
        "command": "code-graph.showGraphForCommit",
        "title": "Code Graph: Show Graph for Commit",
        "icon": "$(git-commit)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@types/babel__traverse": "^7.20.5"
  }
}
